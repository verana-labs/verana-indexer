    "@tendermint/amino-js": "^0.6.2",
    "@types/parse-uri": "^1.0.2",
    "@types/swagger-ui-dist": "3.30.6",
    "@verana-labs/verana-types": "0.9.1",
    "ajv-formats": "^2.1.1",
    "axios": "^1.12.2",
    "bignumber.js": "^9.3.1",
    "bull": "^4.16.5",
    "bullmq": "^3.16.2",
    "busboy": "^1.6.0",
    "cosmjs-types": "^0.6.1",
    "dayjs": "^1.11.18",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "file-type": "^18.7.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.8.1",
    "is-ipfs": "^8.0.4",
    "knex": "^2.5.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "long": "^5.3.2",
