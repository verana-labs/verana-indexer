{
  "name": "verana-indexer",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "_moduleAliases": {
    "@base": "src/base"
  },
  "scripts": {
    "clean": "rimraf ./dist/",
    "docker": "npm run up && npm run db:migrate:latest && npm run dev",
    "start": "cd dist && node --es-module-specifier-resolution=node ../node_modules/moleculer/bin/moleculer-runner.mjs --env --config ./src/moleculer.config.js",
    "dev": "cross-env NODE_OPTIONS=--loader=ts-node/esm node ./node_modules/moleculer/bin/moleculer-runner.mjs --hot --repl --env --config ./src/moleculer.config.ts",
    "watch": "nodemon",
    "db:migrate:make": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" knex migrate:make --knexfile knexfile.ts -x ts",
    "db:migrate:latest": "cross-env  node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:latest --knexfile knexfile.ts",
    "db:migrate:rollback": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" knex migrate:rollback --knexfile knexfile.ts",
    "db:migrate:test:latest": "env DOTENV_CONFIG_PATH=./.env NODE_ENV=test ts-node --require dotenv/config ./node_modules/.bin/knex migrate:latest",
    "db:migrate:test:rollback": "env DOTENV_CONFIG_PATH=./.env NODE_ENV=test ts-node --require dotenv/config ./node_modules/.bin/knex migrate:rollback",
    "db:seed:make": "env DOTENV_CONFIG_PATH=./.env ts-node --require dotenv/config ./node_modules/.bin/knex seed:make",
    "build": "npm run clean && tsc --project tsconfig.build.json",
    "up": "docker-compose up -d",
    "stop": "docker-compose stop",
    "down": "docker-compose down && npm run docker",
    "prepare": "husky install",
    "jest": "NODE_OPTIONS='--loader ts-node/esm' jest",
    "test": "docker compose up aurad -d && npm run jest -- --verbose --runInBand --forceExit && docker compose stop aurad && docker rm aurad_erascope",
    "test-ci": "npm run jest -- --verbose --runInBand --forceExit",
    "test-watch": "npm run jest --watch",
    "test-coverage": "npm run jest --coverage",
    "lint": "echo 'Running lint for repository' && eslint ."
   },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/moleculer/bin/moleculer-runner.mjs --hot --repl --env --config src/moleculer.config.ts",
    "ext": "ts, js"
  },
  "dependencies": {
    "@aura-nw/aurajs": "0.1.10",
    "@bull-board/api": "^5.1.1",
    "@bull-board/express": "^5.1.1",
    "@cosmjs/cosmwasm-stargate": "^0.30.0",
    "@cosmjs/encoding": "^0.32.3",
    "@cosmjs/json-rpc": "^0.29.5",
    "@cosmjs/proto-signing": "^0.29.5",
    "@cosmjs/stargate": "^0.31.1",
    "@cosmjs/tendermint-rpc": "^0.30.0",
    "@cosmjs/utils": "^0.29.5",
    "@ethereum-sourcify/lib-sourcify": "^1.7.1",
    "@evmos/address-converter": "^0.1.9",
    "@horoscope/evmos-proto": "0.0.8",
    "@horoscope/sei-js-proto": "3.1.9",
    "@osmonauts/lcd": "^0.10.0",
    "@ourparentcenter/moleculer-decorators-extended": "^1.1.1",
    "@shazow/whatsabi": "^0.11.0",
    "@tendermint/amino-js": "^0.6.2",
    "@types/parse-uri": "^1.0.0",
    "ajv-formats": "^2.1.1",
    "aws-sdk": "^2.1377.0",
    "axios": "^1.6.0",
    "bignumber.js": "^9.1.1",
    "bull": "^4.10.2",
    "bullmq": "^3.13.3",
    "busboy": "^1.6.0",
    "cosmjs-types": "^0.6.1",
    "dayjs": "^1.11.9",
    "dotenv": "^16.0.3",
    "ethers": "^6.13.0",
    "file-type": "^18.4.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.2.4",
    "is-ipfs": "^8.0.1",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "long": "^5.2.1",
    "mkdirp": "^1.0.4",
    "moleculer": "^0.14.27",
    "moleculer-auto-openapi": "^1.1.5",
    "moleculer-bull": "^0.3.1",
    "moleculer-db": "^0.8.21",
    "moleculer-web": "^0.10.5",
    "mysql": "^2.18.1",
    "node-fetch": "^3.3.2",
    "objection": "^3.0.1",
    "parse-uri": "^1.0.7",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "redisuri": "^1.1.2",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.5.4",
    "stream-chain": "^2.2.5",
    "stream-json": "^1.7.5",
    "underscore": "^1.13.6",
    "util": "^0.12.4",
    "viem": "^2.14.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.3.0",
    "@jest-decorated/core": "^0.1.7",
    "@jest/globals": "^29.3.1",
    "@osmonauts/lcd": "^0.10.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/jest-when": "^3.5.2",
    "@types/lodash": "^4.14.191",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.19.120",
    "@types/stream-chain": "^2.0.1",
    "@types/stream-json": "^1.7.3",
    "@types/underscore": "^1.11.4",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "ajv-formats": "^2.1.1",
    "cross-env": "^10.0.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "29.7.0",
    "jest-light-runner": "^0.4.1",
    "jest-mock": "29.7.0",
    "lint-staged": "^13.1.0",
    "moleculer-repl": "^0.7.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node",
    "transpileOnly": true,
    "compilerOptions": {
      "module": "NodeNext",
      "moduleResolution": "NodeNext"
    }
  }
}
