    "parse-uri": "^1.0.16",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "protobufjs": "7.5.4",
    "redisuri": "^1.1.2",
    "reflect-metadata": "^0.1.14",
    "semver": "^7.7.3",
    "stream-chain": "^2.2.5",
    "stream-json": "^1.9.1",
    "swagger-ui-dist": "5.29.5",
